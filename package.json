{
  "name": "korapay",
  "version": "1.0.0",
  "description": "Korapay test",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node dist/index.js",
    "start:dev": "nodemon dist/index.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "dev": "concurrently  \"npm:build:dev\" \"npm:start:dev\"",
    "test": "jest ",
    "lint": "tslint -p tsconfig.json -c tslint.json --fix"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "testEnvironment": "node",
        "tsconfig": "tsconfig.json"
      }
    },
    "modulePaths": [
      "<rootDir>"
    ],
    "testMatch": [
      "**/tests/*.+(ts|tsx)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/korapay-engineering/bems-emm-ade.git"
  },
  "keywords": [
    "payment",
    "korapay"
  ],
  "author": "Emmanuel Adebayo",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/korapay-engineering/bems-emm-ade/issues"
  },
  "homepage": "https://gitlab.com/korapay-engineering/bems-emm-ade#readme",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@types/bluebird": "^3.5.26",
    "@types/hapi__joi": "^17.1.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/socket.io": "^2.1.13",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "axios": "^0.19.2",
    "bcrypt": "^5.0.0",
    "bignumber.js": "^9.0.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.9.2",
    "faker": "^5.2.0",
    "helmet": "^3.23.3",
    "http-status-codes": "^1.3.2",
    "joi": "^17.3.0",
    "joi-phone-validator": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
